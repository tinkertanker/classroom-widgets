# Staging Environment Configuration
# Copy this to .env.staging for local Docker testing

# Server Configuration
VITE_SERVER_URL=http://localhost:3001
CORS_ORIGINS=http://localhost:3000,http://localhost

# API Keys (use test/staging keys here)
VITE_SHORTIO_API_KEY=your-staging-api-key
VITE_SHORTIO_BASE_URL=https://short.io

# Feature Flags
VITE_DEBUG=false

# Staging-specific settings
NODE_ENV=staging
STAGING_MODE=true